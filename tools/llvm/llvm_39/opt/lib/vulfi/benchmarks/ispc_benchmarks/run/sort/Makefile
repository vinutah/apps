
all:
	(cd ispc-avx2/data; make)	
	(cd ispc-avx2/ctrl; make)
	(cd ispc-avx2/addr; make)
	(cd ispc-avx2/mask; make)
	(cd ispc-sse4/data; make)	
	(cd ispc-sse4/ctrl; make)
	(cd ispc-sse4/addr; make)
	(cd ispc-sse4/mask; make)
	(cd serial/data; make)	
	(cd serial/ctrl; make)
	(cd serial/addr; make)

clean:
	(cd ispc-avx2/data; make clean)
	(cd ispc-avx2/ctrl; make clean)
	(cd ispc-avx2/addr; make clean)
	(cd ispc-avx2/mask; make clean)
	(cd ispc-sse4/data; make clean)	
	(cd ispc-sse4/ctrl; make clean)
	(cd ispc-sse4/addr; make clean)
	(cd ispc-sse4/mask; make clean)
	(cd serial/data; make clean)
	(cd serial/ctrl; make clean)
	(cd serial/addr; make clean)

