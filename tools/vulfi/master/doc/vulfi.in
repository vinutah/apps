PROJECT_NAME           = "VULFI - An LLVM based Fault Injector"
PROJECT_NUMBER         = @VERSION_MAJOR@.@VERSION_MINOR@.@VERSION_PATCH@
STRIP_FROM_PATH        = @PROJECT_SOURCE_DIR@ \
                         @PROJECT_BINARY_DIR@
INPUT                  = @doxy_main_page@ \
                         @PROJECT_SOURCE_DIR@/include \
                         @PROJECT_SOURCE_DIR@/lib
RECURSIVE              = YES
USE_MDFILE_AS_MAINPAGE = @doxy_main_page@
GENERATE_LATEX   = NO
GENERATE_MAN     = NO
GENERATE_RTF     = NO
CASE_SENSE_NAMES = NO
FILE_PATTERNS    = *.cpp *.h *.hpp
QUIET            = YES
JAVADOC_AUTOBRIEF = NO
SEARCHENGINE     = YES
EXTRACT_ALL = NO
SOURCE_BROWSER = YES
CALL_GRAPH = YES
CALLER_GRAPH = YES
FULL_PATH_NAMES = NO
CLASS_DIAGRAMS = YES
